                "valid_ws_types": ["KBaseFile.PairedEndLibrary", "KBaseAssembly.PairedEndLibrary"]
            }
        },
        {
            "id": "output_contigset_name",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["SPAdes.contigs"],
            "field_type": "text",
