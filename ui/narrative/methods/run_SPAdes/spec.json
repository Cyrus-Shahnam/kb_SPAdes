                {
                    "service_method_output_path": [0],
                    "target_property": "SPAdes_result"
                },
                {
                    "input_parameter": "read_library_name",
                    "target_property": "input_read_library_name"
                },
                {
                    "input_parameter": "output_contigset_name",
                    "target_property": "input_output_contigset_name"
                },
                {
                    "input_parameter": "single_cell",
                    "target_property": "input_single_cell"
