                "valid_ws_types" : [ "KBaseGenomes.ContigSet" ],
                "is_output_name":true
            }
        },
        {
            "id": "single_cell",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [""],
